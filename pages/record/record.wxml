
<block wx:if="{{list.length}}">
    <view class='item' wx:for='{{list}}' wx:key='{{memberId}}'>
        <image class="money icon" src="../../images/money.png" mode="cover"></image>
        <view class="flex1">
            <view class="number">{{item.accountType}} +{{item.disAmount}}</view>
            <view class="time">{{item.addTime}}</view>
        </view>
    </view>
</block>
<view wx:else style="padding:100rpx 0;text-align:center">暂无数据</view>